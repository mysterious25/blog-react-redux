{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "A simple task of React, Redux and React Router",
  "main": "index.js",
  "repository": "git@github.com:mysterious25/blog-react-redux.git",
  "scripts": {
    "build:webpack": "webpack --config webpack.config.js",
    "build": "npm run build:webpack",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "rename-index": "node -e \"require('fs').rename('./build/index.html', './build/200.html', (err) => { console.log(err ? err : 'File renamed!') })\"",
    "predeploy": "npm run build && npm run rename-index",
    "deploy": "surge --project ./build --domain react-redux"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "gh-pages": "^1.0.0",
    "surge": "^0.21.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "babel-preset-stage-1": "^6.1.18",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.0.4",
    "redux-form": "^4.1.3",
    "redux-promise": "^0.5.1"
  }
}
